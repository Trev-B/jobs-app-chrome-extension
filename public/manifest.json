{
  "manifest_version": 3,
  "name": "Job Extension",
  "description": "Allows user to add jobs from any tab.",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+V",
        "mac": "Command+U"
      }
    }
  }
}
